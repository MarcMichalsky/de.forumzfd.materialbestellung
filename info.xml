<?xml version="1.0"?>
<extension key="de.forumzfd.materialbestellung" type="module">
  <file>materialbestellung</file>
  <name>ForumZFD Material Bestellungen</name>
  <description>ForumZFD specific CiviCRM extension for Material Bestellungen</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Erik Hommel</author>
    <email>hommel@ee-atwork.nl</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/ErikHommel/de.forumzfd.materialbestellung</url>
    <url desc="Documentation">https://github.com/ErikHommel/de.forumzfd.materialbestellung</url>
    <url desc="Support">https://github.com/ErikHommel</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-02-14</releaseDate>
  <version>1.5</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <comments>Extension is being tested</comments>
  <requires>
    <ext>de.forumzfd.apiprocessing</ext>
  </requires>
  <civix>
    <namespace>CRM/Materialbestellung</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Materialbestellung_Upgrader</upgrader>
</extension>
